{
    "name": "jonquihote/filament-bob",
    "description": "Use 'make' commands in your filament plugins.",
    "keywords": [
        "filament",
        "generator",
        "cli",
        "filament generator cli"
    ],
    "homepage": "https://github.com/jonquihote/filament-bob",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jonquihote/filament-bob/issues",
        "source": "https://github.com/jonquihote/filament-bob"
    },
    "authors": [
        {
            "name": "Joni Chandra",
            "email": "jonquihote@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bob"
    ]
}
